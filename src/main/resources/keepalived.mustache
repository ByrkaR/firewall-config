! autogenerated

global_defs {
    router_id LVS_DEVEL
}

{{#interfaces}}
vrrp_instance V_{{instance}} {
    state             MASTER
    interface         {{interfaceName}}
    virtual_router_id {{instance}}
    priority          {{priority}}
    advert_int        1

    authentication {
        auth_type PASS
        auth_pass 1111
    }

    virtual_ipaddress {
        {{ip}}/{{netmask}}
    }
}

{{/interfaces}}
